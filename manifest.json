{
    "name":"No Leetcode, No Youtube",
    "short_name": "nlny",
    "description":"You really have to solve a leecode problem to watch youtube.",
    "manifest_version": 2,
    "version":"20.12.28",
    "author":"kim.sardine",
    "homepage_url": "https://github.com/kim-sardine/no-leetcode-no-youtube",
    "icons": {
      "48": "icons/white_48.png",
      "128": "icons/white_128.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts":[
      {
        "matches": ["*://*.youtube.com/*"],
        "js":["utils.js", "youtube.js"],
        "run_at": "document_start"
      },
      {
        "matches": ["*://*.leetcode.com/*"],
        "js":["utils.js", "leetcode.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": [
      "tabs",
      "storage",
      "notifications"
    ]
  }